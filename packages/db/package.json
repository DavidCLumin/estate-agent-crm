{
  "name": "@estate/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:rls": "prisma db execute --schema prisma/schema.prisma --file prisma/migrations/001_init/migration.sql",
    "db:seed": "tsx prisma/seed.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'lint db'"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "argon2": "^0.41.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "prisma": "^5.19.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
